#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/dev/dist/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/dev/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/dev/dist/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/dev/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules/@remix-run/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/@remix-run+dev@2.16.8_@remix-run+react@2.16.8_@remix-run+serve@2.16.8_typescript@5.8.3_vite@6.3.5/node_modules:/Users/adrianbuduna/Desktop/my-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../@remix-run/dev/dist/cli.js" "$@"
fi
